<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="stockwinner.controller.StrategyController">
   <children>
      <HBox spacing="10.0">
         <children>
      
                    <Button onAction="#addStrategy" text="+" />
              <Button onAction="#delStrategy" text="-" />
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>

        <ScrollPane prefHeight="600" prefWidth="600">
            <VBox fx:id="strats">

            </VBox>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
        </ScrollPane>
   </children>
</VBox>
